# work

# Example for vagrant virtual machines
# this is a way to avoid host key checking and always use
# root over a group of ip or names
Host 192.168.33.* *.myapp.dev
  StrictHostKeyChecking no
  UserKnownHostsFile=/dev/null
  User root
  LogLevel ERROR

