! Xterm
! -----
XTerm*vt100.metaSendsEscape:	true
XTerm*font:			9x15
XTerm*urgentOnBell:		true
XTerm*background:		rgb:ff/ff/ff
XTerm*foreground:		rgb:00/00/00
XTerm*utf8:			2

! Misc
! ----
Xft.dpi: 		96
Xcursor.theme: 		Vanilla-DMZ

! URxvt
! -----
URxvt*scrollBar:    false
URxvt.scrollstyle:  plain
URxvt*saveLines:    10000
URxvt*urgentOnBell: true
!URxvt*font:	    -*-terminus-medium-*-*-*-14-*-*-*-*-*-*-*
!URxvt.font:         6x13
URxvt.font:         xft:inconsolata:size=11
!URxvt*fading:	    25
URxvt*fadeColor:    rgb:ee/ee/ee
URxvt*foreground:   rgb:ee/ee/ee
URxvt*background:   rgb:22/22/22
URxvt*cursorColor:  rgb:00/f6/3f

URxvt.perl-ext-common:  default,matcher
URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1 

! black
URxvt*color0:          rgb:00/00/00
! red
URxvt*color1:          rgb:cc/00/00
! green
URxvt*color2:          rgb:00/99/00
! yellow
URxvt*color3:          rgb:99/99/00
! blue
URxvt*color4:          rgb:00/66/ff
! magenta
URxvt*color5:          rgb:99/33/99
! cyan
URxvt*color6:          rgb:00/99/99
! gray
URxvt*color7:          rgb:cc/cc/cc
! darkgray
URxvt*color8:          rgb:55/55/55
! lightred
URxvt*color9:          rgb:ff/00/00
! lightgreen
URxvt*color10:         rgb:00/cc/00
! lightyellow
URxvt*color11:         rgb:cc/cc/00
! lightblue
URxvt*color12:         rgb:33/99/ff
! lightmagenta
URxvt*color13:         rgb:cc/66/ff
! lightcyan
URxvt*color14:         rgb:33/cc/cc
! white
URxvt*color15:         rgb:ff/ff/ff

! XBoard
! ------
XBoard*font:			-*-snap-*-*-*-*-*-*-*-*-*-*-*-*
XBoard*clockFont:		-*-snap-*-*-*-*-*-*-*-*-*-*-*-*
XBoard*coordFont:		-*-snap-*-*-*-*-*-*-*-*-*-*-*-*
XBoard*whitePieceColor:		gray100
XBoard*blackPieceColor:		gray0
XBoard*lightSquareColor:	gray80
XBoard*darkSquareColor:		gray60
XBoard*highlightSquareColor:	gray100
XBoard*premoveHighlightColor:	gray70
